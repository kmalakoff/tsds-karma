{
  "name": "tsds-karma",
  "version": "1.14.2",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "mocha",
    "stack",
    "test",
    "typescript"
  ],
  "homepage": "https://github.com/kmalakoff/tsds-karma",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/tsds-karma.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "source": "src/index.ts",
  "types": "dist/cjs/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "tsds format",
    "prepublishOnly": "tsds validate",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "assert": "1.5.0",
    "cross-spawn-cb": "*",
    "getopts-compat": "^2.2.6",
    "karma": "*",
    "karma-chrome-launcher": "*",
    "karma-mocha": "*",
    "karma-mocha-reporter": "*",
    "karma-sourcemap-loader": "*",
    "karma-webpack": "*",
    "link-unlink": "*",
    "queue-cb": "*",
    "regenerator-runtime": "*",
    "resolve-bin-sync": "*",
    "ts-loader": "*",
    "ts-swc-loaders": "*",
    "tsds-lib": "*",
    "webpack": "*"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "mocha": "*",
    "mocha-compat": "*",
    "module-link-unlink": "*",
    "node-version-use": "*",
    "os-shim": "*",
    "resolve": "*",
    "short-hash": "*",
    "ts-dev-stack": "*",
    "ts-swc-loaders": "*",
    "tsds-config": "*",
    "tsds-lib-test": "*",
    "typescript": "*"
  },
  "engines": {
    "node": ">=0.8"
  }
}
